	"styles": {
		"spacing": {
			"blockGap": "var(--wp--custom--widget--layout--gap)"
		},
		"typography": {
			"fontSize": "var(--wp--preset--font-size--scaler-regular )",
			"lineHeight": "var(--wp--custom--body--typography--line-height)",
			"fontFamily": "var(--wp--custom--body--typography--font-family)"
		},
		"elements" : {
			"h1": {
				"typography": {
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)",
					"fontSize": "var(--wp--preset--font-size--scaler-xlarge )"
				}
			},
			"h2": {
				"typography": {
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)",
					"fontSize": "var(--wp--preset--font-size--scaler-larger )"
				}
			},
			"h3": {
				"typography": {
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)",
					"fontSize": "var(--wp--preset--font-size--scaler-large)"
				}
			},
			"h4": {
				"typography": {
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)",
					"fontSize": "var(--wp--preset--font-size--scaler-medium)"
				}
			},
			"h5": {
				"typography": {
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)",
					"fontSize": "var(--wp--preset--font-size--scaler-regular)"
				}
			},
			"h6": {
				"typography": {
					"fontWeight": "var(--wp--custom--heading--typography--font-weight)",
					"lineHeight": "var(--wp--custom--heading--typography--line-height)",
					"fontSize": "var(--wp--preset--font-size--scaler-small)"
				}
			}
		}
