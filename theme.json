		"custom" : {
			"body": {
				"typography": {
					"lineHeight": 1.5,
					"fontSize" : "clamp(1rem, 0.8130311614730878rem + 0.45325779036827185vw, 1.2rem)",
					"fontFamily": "var(--wp--preset--font-family--sans-serif )"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": 400,
					"lineHeight": 1.125,
					"margin" : {
						"bottom" : "1.618rem"
					}
				}
			},
			"widget" : {
				"layout" : {
					"gap" : "1.618rem"
				}
			}
		}
